# YouTube API Configuration
EXPO_PUBLIC_YOUTUBE_API_KEY=your_youtube_api_key_here
EXPO_PUBLIC_YOUTUBE_API_DEBUG=false
EXPO_PUBLIC_YOUTUBE_API_BASE=https://www.googleapis.com/youtube/v3

# Google OAuth Configuration
# Get this from Google Cloud Console: https://console.cloud.google.com/apis/credentials
# For Expo React Native native sign-in (recommended): use the Web Client ID
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=your_web_client_id.apps.googleusercontent.com

# For expo-auth-session fallback (Expo Go / web): can use your platform client id(s)
EXPO_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id_here.apps.googleusercontent.com

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url_here
EXPO_PUBLIC_SUPABASE_PUBLISHABLE_KEY=your_supabase_anon_key_here

